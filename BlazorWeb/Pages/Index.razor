@page "/"
@attribute [Authorize]

@if (booking != null)
{
	@booking.Reason
}

@if (employee != null)
{
	@employee.Firstname
}

@code {
	Booking booking { get; set; }

	Employee employee { get; set; }

	protected override async Task OnInitializedAsync()
	{
		booking = await _bookingRepo.GetBookingByBookingId(1);

		employee = await _hospitalRepo.GetEmployeeById(4);

	}
}